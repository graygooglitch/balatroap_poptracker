{
  //sets the trackers layout
  //more info: https://github.com/black-sliver/PopTracker/blob/master/doc/PACKS.md#layouts
  "tracker_default": {
    "type": "container",
    "background": "#000000",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "Deck, Stake, Ante Checks",
              "content": {
                "type": "map",
                "maps": [
                  "Deck, Stake, Ante Checks"
                ]
              }
            }
          ]
        }
      ]
    }
  },
  "tracker_horizontal": {
    "type": "container",
    "background": "#000000",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "Deck, Stake, Ante Checks",
              "content": {
                "type": "map",
                "maps": [
                  "Deck, Stake, Ante Checks"
                ]
              }
            }
          ]
        }
      ]
    }
  }
}
